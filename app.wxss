/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.theme_color {
  color: #f24827;
}
view {
  font-size:24rpx;
  color: #444;
}
.line2 {
  display:-webkit-box;
  overflow:hidden;
  white-space:normal!important;
  text-overflow:ellipsis;
  word-wrap:break-word;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
}

